length + unloggedLandings;\n    return toDuration(minutes) + \" (\" + landings + \")\"\n}\n\nlet max = (arr, attr) => Math.max(...arr.map(obj => obj[attr]), 0);\nlet min = (arr, attr) => Math.min(...arr.map(obj => obj[attr]), 0);\nlet maxDuration = (arr) => toDuration(Math.max(...arr.map(obj => toMin(obj.duration)), 0));\n\n\n\nthis.flights[\"Total\"]   = total(this.logTable); //include unlogged\n\nthis.flights[\"Foot\"]    = summarise(this.logTable.filter(row => row.launch == \"Foot\"));\nthis.flights[\"Tow\"]     = summarise(this.logTable.filter(row => row.launch == \"Tow\"));\nthis.flights[\"Tandem\"]  = summarise(this.logTable.filter(row => row.type == \"Tandem\"));\nthis.flights[\"PPG\"]     = summarise(this.logTable.filter(row => row.type == \"PPG\"));\n\n\nthis.flights[\"d300\"]  = this.logTable.filter(row => row.distance >= 300).length;\nthis.flights[\"d200\"]  = this.logTable.filter(row => row.distance >= 200).length;\nthis.flights[\"d100\"]  = this.logTable.filter(row => row.distance >= 100).length;\nthis.flights[\"d50\"]   = this.logTable.filter(row => row.distance >= 50).length;\nthis.flights[\"d20\"]   = this.logTable.filter(row => row.distance >= 20).length;\nthis.flights[\"d10\"]   = this.logTable.filter(row => row.distance >= 10).length;\n\nthis.flights[\"maxDuration\"]   = maxDuration(this.logTable);\nthis.flights[\"maxAltitude\"]   = max(this.logTable, \"maxAltitude\");\nthis.flights[\"maxDistance\"]   = max(this.logTable, \"distance\");\nthis.flights[\"maxClimb\"]      = max(this.logTable, \"maxClimb\");\nthis.flights[\"maxSink\"]       = min(this.logTable, \"maxSink\");\nthis.flights[\"toplandings\"]   = this.logTable.filter(row => row.landing == \"topland\").length;\n\nconsole.groupEnd()"}}}}}}